{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Android",
        "type": "reactnative",
        "request": "launch",
        "platform": "android",
        "target": "simulator",
        "sourceMaps": true,
        "outDir": "${workspaceRoot}/.vscode/.react"
      },
      {
        "name": "Debug iOS",
        "type": "reactnative",
        "request": "launch",
        "platform": "ios",
        "target": "simulator",
        "sourceMaps": true,
        "outDir": "${workspaceRoot}/.vscode/.react"
      },
      {
        "name": "Attach to packager",
        "type": "reactnative",
        "request": "attach",
        "port": 8081,
        "sourceMaps": true,
        "outDir": "${workspaceRoot}/.vscode/.react"
      }
    ]
  }
  